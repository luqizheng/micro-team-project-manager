{
  "name": "project-manager-server",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "node dist/main.js",
    "build": "nest build",
    "start:dev": "nest start --watch",
    "lint": "eslint . --ext .ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/typeorm-data-source.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate src/migrations/$npm_config_name -d src/typeorm-data-source.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/typeorm-data-source.ts",
    "env:dev": "copy .env.development.example .env",
    "env:prod": "copy .env.production.example .env",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --testPathPattern=app.e2e-spec.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/s3-request-presigner": "^3.637.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "mysql2": "^3.9.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "test": "^3.3.0",
    "typeorm": "^0.3.20",
    "typeorm-ts-node-commonjs": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.12.12",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
